          "Aperture",
          "Door",
          "Face",
          "Room",
          "Story",
          "Building"
        ],
        "title": "ParentTypes",
        "type": "string"
      },
      "Plane": {
        "allOf": [
          {
            "$ref": "#/components/schemas/_OpenAPIGenBaseModel"
          },
          {
            "type": "object",
            "properties": {
              "n": {
                "description": "Plane normal as 3 (x, y, z) values.",
                "items": {
                  "type": "number",
                  "format": "double"
                },
                "maxItems": 3,
                "minItems": 3,
                "title": "N",
                "type": "array"
              },
              "o": {
                "description": "Plane origin as 3 (x, y, z) values",
                "items": {
                  "type": "number",
                  "format": "double"
                },
                "maxItems": 3,
                "minItems": 3,
                "title": "O",
                "type": "array"
              },
              "type": {
                "const": "Plane",
                "default": "Plane",
                "title": "Type",
                "type": "string",
                "readOnly": true
              },
              "x": {
                "anyOf": [
                  {
                    "items": {
                      "type": "number"
                    },
                    "maxItems": 3,
                    "minItems": 3,
                    "type": "array"
                  },
                  {
                    "type": "null"
                  }
                ],
                "default": null,
                "description": "Plane x-axis as 3 (x, y, z) values. If None, it is autocalculated.",
                "title": "X"
              }
            },
            "additionalProperties": false,
            "required": [
              "n",
              "o"
            ]
          }
        ],
        "title": "Plane",
        "discriminator": {
          "propertyName": "type"
        }
      },
      "Point3D": {
        "allOf": [
          {
            "$ref": "#/components/schemas/_OpenAPIGenBaseModel"
          },
          {
